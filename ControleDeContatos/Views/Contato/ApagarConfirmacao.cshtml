@model ContatoModel
@{
    ViewData["Title"] = "Confirmação apagar o contato";
}



<div class="text-center">


<h3 class="display-4">Realmente deseja apagar o contato (@Model.Nome)?</h3>

<a class="btn btn-primary btn-block" role="button" asp-controller="Contato" asp-action="Index">Cancelar exclusão</a>
<a class="btn btn-danger btn-block" role="button" asp-controller="Contato" asp-action="Apagar">Confirmar exclusão</a>


</div>


